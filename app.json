{
  "expo": {
    "name": "PhotoSpots",
    "slug": "spotshot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-dark.png",
    "scheme": "spotshot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyA9q2rheHLUpiVxD5CSY7YiDkJgbksZbmE"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.sancus.spotshot",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "SpotShot uses your location to center the map on your current position and show nearby photo spots around you (e.g. popular viewpoints in your area)."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sancus.spotshot",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCEbVv-8yxUZ84vc3I_yWkINowpf62Po0k"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/ios-light.png",
          "dark": {
            "image": "./assets/icons/ios-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "26be4e69-e779-43dd-bee6-5ca18f9af6c8"
      }
    }
  }
}
